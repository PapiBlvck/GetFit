{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": "apps/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/src/**",
      "**/*.config.*",
      "**/*.json"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}


